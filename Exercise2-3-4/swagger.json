      "/operation/addByTwoParams": {
        "get": {
          "tags": ["Addition service"],
          "summary": "Add 2 numbers",
          "parameters": [
            {
              "name": "number1",
              "in": "query",
              "description": "The first number"
            },
            {
              "name": "number2",
              "in": "query",
              "description": "The second number"
            }
          ],
          "responses": {
            "200": {
              "description": "This service allows to add two numbers together"
            }
          }
        }
      },
